{
	"name": "agency dataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dsGen2AgencyMasters",
						"type": "DatasetReference"
					},
					"name": "datalakeagency"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sql_nyc_payroll_agency_md",
						"type": "DatasetReference"
					},
					"name": "dbagency"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          AgencyID as string,",
				"          AgencyName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> datalakeagency",
				"datalakeagency sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          AgencyID as string,",
				"          AgencyName as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> dbagency"
			]
		}
	}
}